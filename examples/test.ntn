using std::IO
using std::MAP

fn main:
    set my_list: ("First", "Second", "Third", "Foruth", "Fifth")
    set my_colors: (Red: "red", Blue: "blue", Green: "green")

    ; Example foreach iteration through my_colors
    IO.println "Iteration of 'my_colors':"
	foreach &key, &value, $my_colors:
	    IO.print "Key: "
	    IO.print $key     ; Red, Blue, Green
		IO.print " = "
		IO.println $value   ; red, blue, green
	end

	; Example foreach iteration through my_list
	IO.println "\nIteration of 'my_list':"
	foreach &position, &value, $my_list:
	    IO.print "Position: "
	    IO.print $position    ; 0, 1, 2, etc
		IO.print " = "
		IO.println $value       ; First, Second, Third, etc
	end
end
