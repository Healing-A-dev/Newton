using std::IO

; Factorial function
fn factorial:
    @args count
    if $count < 2:
        return $count
    else:
        return $count * factorial ($count - 1)
    end
end

; Main function
fn main:
    set res_result: factorial 5
    IO.print "The result of factorial 5 is: "
    IO.println $res_result
end
