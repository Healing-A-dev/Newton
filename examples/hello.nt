using std::IO
using std::File
using std::System

fn main:
		set colors: ("RED", "ORANGE", "YELLOW", "GEEN", "BLUE")
		set colorsLIST: ["RED", "ORANGE", "YELLOW", "GEEN", "BLUE"]
		set colorsDIC: (r: "RED", o: "ORANGE", y: "YELLOW", g: "GEEN", b: "BLUE")

		IO.println "Printing: Map Array:"
		IO.println $colors
		IO.println "----------------------------------\nPrinting Map Dictionary: "
		IO.println $colorsDIC
		IO.println "----------------------------------\nPrinting List: "
		IO.println $colorsLIST
		

		
		IO.println "----------------------------------\nPrinting VALUE FROM MAP: "
		IO.println $colorsLIST{1}
		IO.println $colors{1}
		IO.println $colorsDIC{"r"}


		; PRINTING INTEGERS/FLOATS CAUSES A SEGMENTAION FAULT AS WELL
		; FOR / FOREACH LOOPS ARE ALSO BROKEN
end
