using std::IO
using std::Collection

fn main:
    set colors_by_name: (Bob: "Blue", Gob: "Green", Pob: "Purple")

    IO.println "Length of colors_by_name: " << string Collection.sizeof $colors_by_name

    IO.println "-------------------"
    IO.println "VALUES BEFORE DELETION"
    foreach &key, &value, $colors_by_name:
        IO.println $key << ": " << $value
    ;		IO.println "1"
    end
    IO.println "-------------------"

    Collection.remove $colors_by_name, "Bob"

    IO.println "VALUES AFTER DELETION"
    foreach &key, &value, $colors_by_name:
        IO.println $key << ": " << $value
    end
    IO.println "-------------------"
end
